{
  "name": "feedback",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "compile": "gulp js css",
    "watch": "gulp watch",
    "lint": "eslint .",
    "html-lint": "gulp lint-all-html",
    "pretest": "eslint . && npm run compile && NODE_ENV=test npm run db-migrate",
    "test": "NODE_ENV=test mocha && echo 'Tests against app mounted at /feedback:' && EXPRESS_BASE_PATH=/feedback NODE_ENV=test mocha && npm run html-lint",
    "snyk": "snyk test",
    "db-migrate": "knex migrate:latest --knexfile db/knexfile.js",
    "db-rollback": "knex migrate:rollback --knexfile db/knexfile.js",
    "heroku-postbuild": "npm install --dev && npm run compile && npm prune --production",
    "create-migration": "knex migrate:make --knexfile db/knexfile.js $MIGRATION"
  },
  "// NOTE": [
    "^Pinned dependencies are actually devDependencies, which are only",
    "included so that they are available in the Heroku build pipeline"
  ],
  "dependencies": {
    "body-parser": "~1.13",
    "bookshelf": "^0.9.5",
    "commander": "^2.9.0",
    "consolidate": "~0.14",
    "cookie-parser": "~1.3",
    "express": "^4.14.0",
    "font-awesome": "^4.6.3",
    "govuk-elements-sass": "^1.1.4",
    "govuk_frontend_toolkit": "^4.10.0",
    "hogan.js": "~3.0",
    "i18n": "^0.8.2",
    "jquery": "^3.0.0",
    "knex": "^0.11.5",
    "lmt-utils": "~0.1.4",
    "lodash": "^4.17.4",
    "moment": "^2.14.1",
    "morgan": "^1.6.1",
    "node-uuid": "~1.4",
    "npm-git-secrets": "^1.0.1",
    "pg": "^4.5.5",
    "regenerator-transform": "^0.9.8",
    "safe-buffer": "^5.0.1",
    "serve-favicon": "~2.3",
    "uuid": "^3.1.0",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.9.0",
    "browserify": "^13.0.1",
    "chai": "^3.5.0",
    "debug": "~2.2",
    "eslint": "^2.9.0",
    "eslint-config-airbnb-base": "^3.0.1",
    "eslint-plugin-import": "^1.8.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-plumber": "^1.1.0",
    "gulp-sass": "^2.3.1",
    "gulp-streamify": "^1.0.2",
    "gulp-uglify": "^1.5.3",
    "gulp-util": "^3.0.7",
    "knex-cleaner": "^1.1.3",
    "mocha": "^2.4.5",
    "npm-git-secrets": "^1.0.4",
    "snyk": "^1.19.1",
    "vinyl-source-stream": "^1.1.0",
    "zombie": "^4.2.1"
  },
  "engines": {
    "node": "6.11.1",
    "npm": "5.2.0"
  }
}
